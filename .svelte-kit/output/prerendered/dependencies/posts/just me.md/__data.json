ld digger._",0.8008167263635343,[197,198,199,201,205,207,210,213,216,218,220,222,224,226,228,231,233,235],{"content":157,"parent":6,"score":7},{"content":155,"parent":6,"score":158},{"content":188,"parent":83,"score":200},0.8584851195430516,{"content":202,"parent":203,"score":204},"working the web","posts/doing.md",0.8517735840989192,{"content":175,"parent":176,"score":206},0.841792683859294,{"content":208,"parent":30,"score":209},"- the need to create",0.8252464273708194,{"content":211,"parent":83,"score":212},"personal things inspire",0.8237137424619562,{"content":214,"parent":34,"score":215},"So. Need to carry on and make the best of my time.",0.82139220800408,{"content":68,"parent":69,"score":217},0.8195891688080771,{"content":75,"parent":76,"score":219},0.8190302380740372,{"content":92,"parent":14,"score":221},0.8153248904117956,{"content":46,"parent":30,"score":223},0.8108242457527624,{"content":170,"parent":34,"score":225},0.808950025207907,{"content":162,"parent":62,"score":227},0.8038307563806086,{"content":229,"parent":10,"score":230},"that things I need",0.8037421876521779,{"content":179,"parent":180,"score":232},0.8029545842573224,{"content":167,"parent":62,"score":234},0.801848393822395,{"content":194,"parent":26,"score":236},0.8012549012119478,"2019-12-02T18:26:10.358Z",""],"uses":{"params":["pathName"]}}]}
